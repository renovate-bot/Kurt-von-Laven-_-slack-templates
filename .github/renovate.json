{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "ignorePresets": [":semanticPrefixFixDepsChoreOthers"],
  "packageRules": [
    {
      "matchDepNames": ["python"],
      "semanticCommitType": "fix"
    },
    {
      "matchPaths": [".github/workflows/notify-*.yaml"],
      "semanticCommitType": "fix"
    }
  ]
}
